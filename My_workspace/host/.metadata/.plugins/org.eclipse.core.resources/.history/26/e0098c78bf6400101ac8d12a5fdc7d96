#include <stdio.h>
int main(void)
{
	char ascii1,ascii2,ascii3,ascii4,ascii5,ascii6;

	printf("Input 6 characters:\n");
	fflush(stdout);
	scanf("%c %c %c %c %c %c", &ascii1,&ascii2,&ascii3,&ascii4,&ascii5,&ascii6);
	printf("%u %u %u %u %u %u\n",ascii1,ascii2,ascii3,ascii4,ascii5,ascii6);
	fflush(stdout);
	printf("Press enter key button to exit application");
	fflush(stdout);
	while(getchar() != '\n')
	{

	}
	getchar();
	return 0;
}

