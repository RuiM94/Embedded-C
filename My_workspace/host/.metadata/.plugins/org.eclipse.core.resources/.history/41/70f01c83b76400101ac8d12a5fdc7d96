#include <stdio.h>

int main (void)
{
	char variable = 100;
	int* address2 = &variable;
	printf("This is the value of variable: %d\n", variable);
	fflush(stdout);
	printf("This is the all bytes of address: %x\n", address2);
	fflush(stdout);
	char* address = &variable;
	char data = *address;
	printf("This is the 1st byte of address: %x\n",data);
	fflush(stdout);




}
