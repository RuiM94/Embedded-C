#include <stdio.h>

int main (void)
{
	char variable = 100;

	printf("This is the value of variable: %d\n", variable);
	fflush(stdout);
	printf("This is the all bytes of address %p\n", &variable);
	fflush(stdout);
	char* address = &variable;
	char data = *address;
	printf("This is the 1st byte of address: %o %x %u %d\n",data,data, data, data);
	fflush(stdout);
	*address = 65;
	printf("This is the new value of variable: %d\n", variable);
	fflush(stdout);


}
