#include <stdio.h>
int main(void)
{
	float a, b, c, d;

	printf("Input first number:\n");
	scanf("%d", &a);
	printf("Input second number:\n");
	scanf("%d", &b);
	printf("Input third number:\n");
	scanf("%d", &c);
	printf("Your average is:". (a+b+c)/3);

	return 0;
}
