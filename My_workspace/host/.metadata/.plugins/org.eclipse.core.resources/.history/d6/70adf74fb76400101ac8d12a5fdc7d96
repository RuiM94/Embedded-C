#include <stdio.h>

int main (void)
{
	char variable = 100;

	printf("This is the value of variable: %d", &variable);
	fflush(stdout);
	char* address = &variable;
	char data = *address;
	printf("This is the 1st byte of address: %x",data);
	fflush(stdout);




}
