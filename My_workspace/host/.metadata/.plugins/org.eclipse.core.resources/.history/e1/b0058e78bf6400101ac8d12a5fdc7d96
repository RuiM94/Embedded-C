#include <stdio.h>
int main(void)
{
	float a, b, c;

	printf("Input your 3 numbers:\n");
	fflush(stdout);
	scanf("%f %f %f", &a,&b,&c);

	printf("Your average is: %f\n", (a+b+c)/3);
	fflush(stdout);
	printf("Press enter key button to exit application");
	while(getchar()!= '\n')
	{

	}
	getchar();
	return 0;
}

