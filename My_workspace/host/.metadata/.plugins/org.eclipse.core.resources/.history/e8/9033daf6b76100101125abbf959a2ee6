#include <stdio.h>
int main(void)
{
	float a = 0;
	float b = 0;
	float c = 0;

	printf("Please write your 3 numbers to make an average:");
	scanf("%f %f %f", &a, &b, &c);
	float d = 0;
	d = a+b+c;
	d = d/3;
	printf("\nYour average is: %f", d);


	return 0;
}
