#include <stdio.h>

int main(void)
{
	unsigned char data = (unsigned char) (0x87+0xFF00);
	unsigned int data2 = 0x87+0xFF00;
	float result = (float) 80 / 3 ;

	printf("Data : %u result : %f \n",data, result);
	printf("%u", data2);

	return 0;
}
