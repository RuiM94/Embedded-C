#include <stdint.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

int32_t *ClockControlRegisterAddr = (int32_t*)0x40023830;
int32_t *GPIODModeRegister = (int32_t*)0x40020C00;
int32_t *GPIODOutputDataRegister = (int32_t*)0x40020C14;



int main(void)
{
	//Turn Clock on
	*ClockControlRegisterAddr |= (1<<3);
	//Turn Mode Registor on
	*GPIODModeRegister &= (~(3<<24));
	//Turn the mode register to output
	*GPIODModeRegister |= (1<<24);
	//Turn On LED
	*GPIODOutputDataRegister |= (1<<12);
	//Turn off LED
	for(int32_t i = 0; i<200000;i++){
	*GPIODOutputDataRegister &= (0<<12);
	}
	//Turn on LED
	for(int32_t i = 0; i<200000;i++){
	*GPIODOutputDataRegister |= (1<<12);
	}
    /* Loop forever */
	for(;;){
		;
	}
}
